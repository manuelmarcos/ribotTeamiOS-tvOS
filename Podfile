source 'https://github.com/CocoaPods/Specs.git'
# Podfile

def shared_pods
    pod 'Alamofire', '~> 3.0'
    pod 'AlamofireImage', '~> 2.0'
end
 
target 'ribotTeam' do
    platform :ios, '9.0'
    use_frameworks!
    shared_pods
end
 
target 'ribotTeamtvOS' do
    platform :tvos, '9.0'
    use_frameworks!
    shared_pods
end